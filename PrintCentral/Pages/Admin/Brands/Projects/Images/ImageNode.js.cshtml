//<script>

    var ImageNode = function (projectId) {
        TreeNode.Extend(this, "ImageNode", 0, "@g["Images"]");
		this.ProjectID = projectId;
        this.CanEdit = false;
		this.Expand();
	};

	ImageNode.prototype = {
		constructor: ImageNode,

		OnDoubleClick: function (e) {
			var self = this;
			AppContext.LoadJS("/Admin/Brands/Projects/Images/ImageList.js").then(() => {
				var view = new ImageList(self.ProjectID);
				view.Show(self.getViewContainer());
			});
		}
    };

//</script>