	var PrinterTransferDialog = function (printerid) {
		FormView.Extend(this, "@g["Change printer company"]", `/admin/Locations/Printers/PrinterTransferDialog?id=${printerid}`);
    };

    PrinterTransferDialog.prototype = {
		constructor: PrinterTransferDialog,

		OnLoad: function () {
			this.LoadLocations();
		},

		LoadLocations: function (e) {
			var self = this;
			AppContext.Locations.GetByCompanyID(this.model.CompanyID, function (data) {
				AppContext.LoadComboBox(self.elements.LocationID, data, "ID", "Name");
			});
        }
    };
//</script>