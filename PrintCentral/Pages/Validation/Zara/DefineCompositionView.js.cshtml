    LoadCompoPreview: function () {
        var self = this
        const d = document
        const $previewfontsize = d.querySelector(".fiber-font-size")
        const $wordReplace = d.querySelector(".fiber-separator-type")
        const labelSize = d.querySelector(".fiber-label-size")
        const additionalLabelSize = d.querySelector(".additional-label-size")
        const labelsContainerMarginWidth = 100;

               
              

        self.LabelFitHelper.checkDefaultValues()
        let $labelsContanier = d.querySelector(".labels-container")
        if ($labelsContanier) {
            $labelsContanier.innerHTML = null
        } else {
            $labelsContanier = d.createElement("div")

        }





        self.CompoPreview.Compo = []
        self.CompoPreview.Percent = []
        self.CompoPreview.Leather = []
        self.CompoPreview.Additionals = []
        const fontSize = -1
        const wordReplaceType = $wordReplace.value
        let fiberTextLinesArray = []
        let fiberPercentLinesArray = []
        let AdditionalTextLinesArray = []
        self.CompoPreview.ProjectID = this.options.Selection.ProjectID;
        self.CompoPreview.OrderID = this.options.CompositionDefined.OrderID
        self.CompoPreview.OrderGroupID = this.options.Selection.OrderGroupID;
        AppContext.Wizard.GetCompoPreview(this.options.Selection.ProjectID,
            self.CompoPreview.OrderID,
            this.options.Selection.OrderGroupID,
            this.CompoPreview.Id,
            this.CompoPreview.Id > 0,
            self.CompoPreview.Filling_Weight_Id, 
            self.elements.FillingWeight[0].value,
            0,
            self.options.CompositionDefined.ArticleID,
            function (data) {
                if (!data.Success) {
                    console.log(data.Message)
                    return;
                }

                       if (!data.Data || (Array.isArray(data.Data) && data.Data.length < 1)) {
                           console.log('Data is empty');
                           return;
                       }

                let maxLines = data.Data[0].Lines
                self.CompoPreview.MaxLines = maxLines
                self.CompoPreview.AdditionalsCompress = data.Data[0].AdditionalsCompress;
                self.CompoPreview.FiberCompress = data.Data[0].FiberCompress;
                self.CompoPreview.PPIValue = data.Data[0].PPIValue;

                let labelWidthInPixels = self.LabelFitHelper.inchesToPixels(data.Data[0].Width, self.CompoPreview.PPIValue)
                self.elements.ExceptionLocation.val(data.Data[0].ExceptionsLocation)
                additionalLabelSize.value = self.CompoPreview.AdditionalsCompress
                labelSize.value = self.CompoPreview.FiberCompress
                       
                let $divLabelContainer, $divPercentContainer, $divCompoContainer
                let currentPageLine = 0;
                let fiberscount = 0;
                let pendingFiber = false;
                let page = 1;
                let labelContainerHeight = 16 * self.CompoPreview.MaxLines;
                for (i = 1; i <= data.Data[0].CompoData.ComposNumber; i++) {

                    let { percentData, compolines, leatherLines } = self.ReadLabelPreviewConfig(data, self);
                    let percentlines = []

                    percentlines = percentData[0][1].split('\r\n')
                    $divLabelContainer = d.createElement("div")
                    $divLabelContainer.classList.add("label-container")
                    let labelContainerSize = labelWidthInPixels + (labelWidthInPixels * 25 / 100) + labelsContainerMarginWidth + 35
                    $divLabelContainer.style.width = `${labelContainerSize}px`
                    $divLabelContainer.style.height = `${labelContainerHeight}px`
                    $divPercentContainer = self.LabelFitHelper.initLabelElement(i, fontSize, "percent-container")
                    $divCompoContainer = self.LabelFitHelper.initLabelElement(i, fontSize, "compo-container")
                    $divCompoContainer.style.fontSize = "11px";
                    $divCompoContainer.style.letterSpacing = "-1px";

                    let line = 0
                    percentlines.forEach(item => {

                        if (line < self.CompoPreview.MaxLines) {
                            fiberPercentLinesArray.push(item)
                            let numberOfLineString = line.toString();
                            if (leatherLines.indexOf(numberOfLineString) !== -1) {
                                $divPercentContainer.insertAdjacentElement("beforeend", self.LabelFitHelper.createLabelElementContainer("A", "leather-fiber"))
                            } else {
                                $divPercentContainer.insertAdjacentElement("beforeend", self.LabelFitHelper.createLabelElementContainer(item, "percent"))
                            }

                            $divLabelContainer.insertAdjacentElement("afterbegin", $divPercentContainer)
                            line++;

                        }
                    })

                    line = 0;
                    compolines.forEach(item => {
                        if (line < self.CompoPreview.MaxLines) {
                            fiberTextLinesArray.push(item)
                            $divCompoContainer.insertAdjacentElement("beforeend", self.LabelFitHelper.createLabelElementContainer(item, "compo"))
                            $divLabelContainer.insertAdjacentElement("beforeend", $divCompoContainer)
                            line++;
                        }
                    })
                    self.CreateLabelContainer($divLabelContainer, $divPercentContainer, $divCompoContainer, labelWidthInPixels, labelsContainerMarginWidth, $labelsContanier, self, page);
                    page++;
                }

                if (data.Data[0].CompoData.AdditionalsNumber > 0) {
                    self.DisableAdditionalCompresion(false)

                    for (i = 1; i <= data.Data[0].CompoData.AdditionalsNumber; i++) {
                        let additionalData = Object.entries(data.Data[0].CompoData).filter(([key]) => key.startsWith(`AdditionalPage${i}`));
                        $divCompoContainer = self.LabelFitHelper.initLabelElement(i, fontSize, "compo-container")
                        let additionalLines = []
                        $divCompoContainer.style.fontSize = "11px";
                        $divCompoContainer.style.letterSpacing = "-1px";
                        additionalLines = additionalData[0][1].split('\r\n')


                        $divLabelContainer = d.createElement("div")
                        $divLabelContainer.classList.add("label-container")
                        let labelContainerSize = labelWidthInPixels + (labelWidthInPixels * 25 / 100) + labelsContainerMarginWidth + 35
                        $divLabelContainer.style.width = `${labelContainerSize}px`
                        $divLabelContainer.style.height = `${labelContainerHeight}px`
                        let line = 0;
                        if (additionalLines) {

                            additionalLines.forEach(item => {
                                if (line < self.CompoPreview.MaxLines) {
                                    AdditionalTextLinesArray.push(item)
                                    $divCompoContainer.insertAdjacentElement("beforeend", self.LabelFitHelper.createLabelElementContainer(item, "compo"))
                                    $divCompoContainer.style.width = labelContainerSize
                                    $divLabelContainer.insertAdjacentElement("beforeend", $divCompoContainer)
                                }
                                line++;

                                self.CreateLabelContainer($divLabelContainer, $divPercentContainer, $divCompoContainer, labelWidthInPixels, labelsContainerMarginWidth, $labelsContanier, self, page, false);
                            })
                        }
                        page++;

                    }
                } else {
                    self.DisableAdditionalCompresion(true)
                }

                self.CompoPreview.Additionals = AdditionalTextLinesArray;
                self.CompoPreview.Compo = fiberTextLinesArray;
                self.CompoPreview.Percent = fiberPercentLinesArray;
                self.CompoPreview.MaxLines = maxLines

                self.ExtractFillingWeightValue()
                       

            })
    },

    ExtractFillingWeightValue: function () {
        let self = this
        let texto = self.CompoPreview.Compo.filter(t => t.includes("PESO DEL RELLENO"))
        if (texto && texto.length > 0) {
            self.SetFillingWeightText(texto[0])
        }
    },

    DisableAdditionalCompresion: function (disabled) {
        const d = document
