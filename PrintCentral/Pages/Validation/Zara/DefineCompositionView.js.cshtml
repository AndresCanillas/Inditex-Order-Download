    PreviewCompoMaker: function (data) {
        var self = this;
        const d = document
        const $previewfontsize = d.querySelector(".fiber-font-size")
        const $wordReplace = d.querySelector(".fiber-separator-type")
        const labelSize = d.querySelector(".fiber-label-size")
        const additionalSize = d.querySelector(".additional-label-size")
        const labelsContainerMarginWidth = 100;
        const fontSize = -1
        const wordReplaceType = $wordReplace.value
        //       labelSize.value = data.Data[0].FiberCompress;
        //       additionalSize.value = data.Data[0].AdditionalsCompress;

        // if (data.Data[0].FiberCompress > -1)
        // {
        //    labelSize.value = data.Data[0].FiberCompress;
        // }

        if (!labelSize.value) {
            labelSize.value = 0
        } else {
            if (labelSize.value > 100) {
                labelSize.value = 100
            }
        }
        self.CompoPreview.FiberCompress = labelSize.value; 
        self.CompoPreview.ProjectID = this.options.Selection.ProjectID;
        self.CompoPreview.OrderID = this.options.CompositionDefined.OrderID
        self.CompoPreview.OrderGroupID = this.options.Selection.OrderGroupID;
        //console.log("data", data)
        if (!data.Data[0]) {
            return;
        }
        let maxLines = data.Data[0].Lines
        self.CompoPreview.MaxLines = maxLines
        self.CompoPreview.Width = data.Data[0].Width
        let $divLabelContainer, $divPercentContainer, $divCompoContainer, $formLabelContainer, $form, $inputNumber, $button
        let labelWidthInPixels = self.LabelFitHelper.inchesToPixels(data.Data[0].Width)
        let labelWidthInPixelAdditional = self.LabelFitHelper.inchesToPixels(data.Data[0].WidthAdditional)

        self.CompoPreview.Compo = new Array(500).fill("")
        self.CompoPreview.Percent = new Array(500).fill("")
        self.CompoPreview.Leather = new Array(500).fill("")

        let fiberPercentLineNumber = []
        if (labelSize.value > 0) {
            labelWidthInPixels = labelWidthInPixels + (labelWidthInPixels * labelSize.value / 100)
        }
        let absoluteCurrentLine = 0, page = 0, currentPageLine = 0

        data.Data[0].CompositionText.forEach(fiber => {

            let fiberlines = self.LabelFitHelper.splitFiber(fontSize, wordReplaceType, fiber.Text, labelWidthInPixels)

            // if ((fiberlines.length + currentPageLine > maxLines) || (fiber.Percent === "" && fiber.FiberType==="TITLE")) {
            if ((!self.FitNextFiberInCurrentPage(fiber, fiberlines.length, maxLines, currentPageLine))
                || (!self.FitNextTitleInCurrentPage(fiber, fiberlines.length, maxLines, currentPageLine, fontSize, wordReplaceType, labelWidthInPixels))) {

                if (currentPageLine > 0) {
                    let offset = currentPageLine - maxLines < 0 ? 0 : currentPageLine - maxLines
                    if (absoluteCurrentLine > 0 && page == 0) {
                        page++
                        absoluteCurrentLine = (page * maxLines) + offset
                    } else {

                        if (page * maxLines > absoluteCurrentLine) {
                            absoluteCurrentLine = (page * maxLines) + offset
                            page++
                        } else {
                            page++
                            absoluteCurrentLine = (page * maxLines) + offset
                        }
                    }
                    currentPageLine = offset
                }

            }

            self.CompoPreview.Percent[absoluteCurrentLine] = fiber.Percent

            if (fiber.FiberType == "1") {

                self.CompoPreview.Leather[absoluteCurrentLine + 1] = "1"
            }
            fiberlines.forEach(line => {

                self.CompoPreview.Compo[absoluteCurrentLine] = line
                //console.log("Lines", currentPageLine, absoluteCurrentLine, line)
                absoluteCurrentLine++;
                currentPageLine++;
            })
        })

        if (page == 0) {
            page = 1;
        }
        page = Math.ceil(absoluteCurrentLine / maxLines);
               

        self.CompoPreview.Compo.splice(page * maxLines)
        self.CompoPreview.Percent.splice(page * maxLines)
        self.CompoPreview.Leather.splice(page * maxLines)

        //console.log(self.CompoPreview.Compo)
        self.ExtractFillingWeightValue()

    },


    PreviewAdditionalMaker: function (data) {
        let self = this
        const d = document
