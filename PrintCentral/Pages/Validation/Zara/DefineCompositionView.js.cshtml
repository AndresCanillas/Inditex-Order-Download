    DisablePreviewContainer: function (disable) {

        let div = document.getElementById("PreviewContainer");

        if (div) {

            div.disabled = disable

            let elementosHijos = div.getElementsByTagName("*");
            for (let i = 0; i < elementosHijos.length; i++) {
                elementosHijos[i].disabled = disable;
            }

            div.style.opacity = !disable ? "1" : "0.5";
            div.style.pointerEvents = !disable ? "auto" : "none";
        }
    },

    GetComboOptionsTpl: function (dataList, selectFirstOptionText, valueField, textField) {
        var self = this;

        if (!self.CompoArticles) self.CompoArticles = [];

        var options = [];

        if (selectFirstOptionText) {
            options.push(`<option value="0">${selectFirstOptionText}</option>`);
        }

        if (!textField) textField = valueField;

        if (!Array.isArray(textField)) textField = [textField];

        dataList.forEach((value, idx, arr) => {


            var optText = [];
            textField.forEach((fldName) => { optText.push(value[fldName]); })

            var opt = `<option value="${value[valueField]}">${optText.join(' / ')}</option>`
            options.push(opt);
        });

        var allOptions = options.join('');

        return allOptions;

    },

