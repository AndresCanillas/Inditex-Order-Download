            if (additionalSize.value > 0) {
                labelWidthInPixelAdditional = labelWidthInPixelAdditional + (labelWidthInPixelAdditional * additionalSize.value / 100)
            }

            let currentAdditionalPageLine = 0

            data.Data[0].CareInstructionsText
            .filter(f => f.FiberType == '@CareInstructionCategory.ADDITIONAL')
            .forEach(record => {
                var addlines = self.LabelFitHelper.splitAdditional(fontSize, record.Text, labelWidthInPixelAdditional);
                addlines.forEach((line, index) => {
                    self.CompoPreview.Additionals.push(line);
                    currentAdditionalPageLine++;
                });
            })

            // completar la hoja con saltos
            if (currentAdditionalPageLine % maxLines != 0) {

                let pendingToAdd = maxLines - (currentAdditionalPageLine % maxLines)

                for (let blankLine = 0; blankLine < pendingToAdd; blankLine++) {
                    self.CompoPreview.Additionals.push("");
                    currentAdditionalPageLine++;
                }
            }

            //console.log(self.CompoPreview.Additionals)
        } else {
            self.DisableAdditionalCompresion(true)
            //self.CompoPreview.Additionals = []
        }


    },

