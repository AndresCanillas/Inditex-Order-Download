        $form.appendChild($button)
        return $form
    },

    checkDefaultValues: () => {

        const d = document
        //const $previewfontsize = d.querySelector(".fiber-font-size")
        const $wordReplace = d.querySelector(".fiber-separator-type")
        const labelSize = d.querySelector(".fiber-label-size")

        if ($wordReplace.value == -1) {
            $wordReplace.value = 3
        }
        //if ($previewfontsize.value == -1) {
        //    $previewfontsize.value = 13;
        //}
    },

    splitAdditional: (fontSize, additional, maxSize = 380) => {

        let additionalArray = []
        let additionalWords = additional.replace(/-/g, ' -').split(' ');
        const d = document
        const canvas = d.createElement("canvas")
        const context = canvas.getContext("2d")
        const fontFamily = 'Arial, Helvetica, sans-serif';
        context.font = -1 + 'px ' + fontFamily;
        let pixelsUsed = 0
        let line = ""

        additionalWords.forEach(word => {

            word = word + ' '

            let wordMetrics = 0
            wordMetrics = context.measureText(word)
            if (wordMetrics.width + pixelsUsed < maxSize) {
                pixelsUsed += wordMetrics.width
                line += word

            } else {

                if (line.substring(0, line.length - 1).length > 0) {
                    let finalLine = line.substring(0, line.length - 1).replace(/ -/g, '-');
                    additionalArray.push(finalLine)
                    line = word
                    pixelsUsed = wordMetrics.width
                }
            }
        })
        if (line.substring(0, line.length - 1).length > 0) {
            let finalLine = line.substring(0, line.length - 1).replace(/ -/g, '-');
            additionalArray.push(finalLine)

        }

        return additionalArray


    },

    splitFiber: (fontSize, wordReplaceType, fiber, maxSize = 380) => {
        let fiberArray = []
        let fiberWords = fiber.split(' ')
        const d = document
        const canvas = d.createElement("canvas")
        const context = canvas.getContext("2d")
        const fontFamily = 'Arial, Helvetica, sans-serif';
        context.font = -1 + 'px ' + fontFamily;
        let pixelsUsed = 0
        let line = ""


        const wordReplace = (word, wordReplaceType) => {

            if (wordReplaceType == 3) {
                output = word.trim()
                if (word.trim().endsWith('-')) {
                    return word;
                } else {
                    return word + ' '
                }
            }


            if (wordReplaceType == 1) {
                return this.LabelFitHelper.prefix(this.LabelFitHelper.withoutBlankSpaces(word))
            }

            if (wordReplaceType == 2) {
                return this.LabelFitHelper.sufix(this.LabelFitHelper.withoutBlankSpaces(word))
            }
            return word + ' ';
        }


        fiberWords.forEach(word => {
            word = wordReplace(word, wordReplaceType)

            let wordMetrics = 0
            wordMetrics = context.measureText(word)
            if (wordMetrics.width + pixelsUsed < maxSize) {
                pixelsUsed += wordMetrics.width
                line += word

            } else {

                if (line.substring(0, line.length - 1).length > 0) {
                    fiberArray.push(line.substring(0, line.length - 1))
                    line = word
                    pixelsUsed = wordMetrics.width
                }
            }
        })
        if (line.substring(0, line.length - 1).length > 0) {
            fiberArray.push(line.substring(0, line.length - 1))

        }

        return fiberArray
    }

}




