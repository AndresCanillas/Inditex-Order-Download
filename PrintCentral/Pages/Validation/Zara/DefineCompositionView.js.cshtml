                        page++;
                    }

                    self.CompoPreview.Percent[absoluteCurrentLine] = fiber.Percent

                    if (fiber.FiberType == "1") {
                        self.CompoPreview.Leather[absoluteCurrentLine + 1] = "1"
                    }
                    fiberlines.forEach(line => {

                        self.CompoPreview.Compo[absoluteCurrentLine] = line
                        absoluteCurrentLine++;
                        currentPageLine++;
                    })
                })


                let pageAdditionals = 0;
                if (data.Data[0].CompoData.AdditionalsNumber > 0) {
                    let absoluteAdditionalCurrentLine = 0
                    if (additionalSize.value > 0) {
                        labelWidthInPixelAdditional = labelWidthInPixelAdditional + (labelWidthInPixelAdditional * additionalSize.value / 100)
                    }

