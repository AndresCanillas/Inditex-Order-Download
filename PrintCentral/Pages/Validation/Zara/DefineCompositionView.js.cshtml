    SplitArray: function (array, size) {
        let resultado = [];
        for (let i = 0; i < array.length; i += size) {
            resultado.push(array.slice(i, i + size));
        }
        return resultado;
    },

    DrawCompoPreview() {
        const d = document
        const fontSize = -1
