                switch (self.ProjectData.TemplateConfiguration) {


                    case @((int)CITemplateConfig.Disabled) :
                        self.LoadCategories();
                        break;

                    case @((int)CITemplateConfig.Enabled) :
                        self.elements.TemplatesContainer.show();
