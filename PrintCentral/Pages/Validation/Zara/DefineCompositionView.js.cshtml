    FitLabel: function (labelid, labelWidth, labelMarginWidth) {

        var self = this
        const d = document
        let label = labelid.toString();
        const $previewfontsize = d.querySelector(".fiber-font-size")
        const $wordReplace = d.querySelector(".fiber-separator-type")



        let $maxSize = d.querySelector(`input[type="number"][data-pagenumber="${label}"]`)

        if ($wordReplace.value == -1) {
            $wordReplace.value = 3
        }

        const fontSize = -1
        const wordReplaceType = $wordReplace.value
        let fiberTextLinesArray = []
        let fiberPercentLinesArray = []
        let $divLabelContainer, $divPercentContainer, $divCompoContainer, $formLabelContainer, $form, $inputNumber, $button
        let currentPageLine = 0;
        let fiberscount = 0;
        let pendingFiber = false;
        let leatherLines = self.CompoPreview.Leather[labelid]

        $divCompoContainer = d.querySelector(`.compo-container[data-pagenumber="${label}"]`)
        $divPercentContainer = d.querySelector(`.percent-container[data-pagenumber="${label}"]`)

        $divCompoContainer.innerHTML = null;
        $divPercentContainer.innerHTML = null;
        let $labelsContanier = d.querySelector(".labels-container")
        let startFiber = self.CompoPreview.MaxLines * (labelid - 1)
        let finnishFiber = self.CompoPreview.MaxLines * labelid

        let joinFiber = self.LabelFitHelper.joinFiber(self.CompoPreview.Compo.slice(startFiber, finnishFiber),
            self.CompoPreview.Percent.slice(startFiber, finnishFiber))

        let labelFitLength = labelWidth + (labelWidth * $maxSize.value / 100)
        let labelWidthInPixels = self.LabelFitHelper.mmToPixels(labelFitLength, 96)

        let currentLine = 0
        fiberscount = 0

        joinFiber.fiberRestore.forEach(item => {
            let fiberlines = self.LabelFitHelper.splitFiber(fontSize, $wordReplace.value, item, labelFitLength)

            let currentFiberLine = 0
            fiberlines.forEach(line => {

                let numberOfLineString = currentLine.toString()

                if (currentFiberLine === 0) {
                    self.CompoPreview.Percent[startFiber + currentLine] = joinFiber.fiberPercentRestore[fiberscount]
                    $divPercentContainer.insertAdjacentElement("beforeend",
                        self.LabelFitHelper.createLabelElementContainer(joinFiber.fiberPercentRestore[fiberscount], "percent"))


                    if (self.CompoPreview.Leather[labelid - 1]) {
                        let indice = self.CompoPreview.Leather[labelid - 1].indexOf(numberOfLineString)

                        if (indice !== -1) {
                            let newLeatherPosition = self.CompoPreview.Leather[labelid - 1].replace(numberOfLineString, (currentLine + 1).toString())
                            self.CompoPreview.Leather[labelid - 1] = newLeatherPosition
                        }

                    }

                } else {


                    if (self.CompoPreview.Leather[labelid - 1] && self.CompoPreview.Leather[labelid - 1].indexOf(numberOfLineString) !== -1) {
                        self.CompoPreview.Percent[startFiber + currentLine] = ""
                        $divPercentContainer.insertAdjacentElement("beforeend",
                            self.LabelFitHelper.createLabelElementContainer("A", "leather-fiber"))

                    } else {
                        self.CompoPreview.Percent[startFiber + currentLine] = ""
                        $divPercentContainer.insertAdjacentElement("beforeend",
                            self.LabelFitHelper.createLabelElementContainer("", "percent"))
                    }
                }
                self.CompoPreview.Compo[startFiber + currentLine] = line
                $divCompoContainer.insertAdjacentElement("beforeend", self.LabelFitHelper.createLabelElementContainer(line, "compo", labelWidth + labelMarginWidth))
                currentLine++;
                currentFiberLine++
            })

            fiberscount++;
        })

        for (i = startFiber + currentLine; i < finnishFiber; i++) {

            self.CompoPreview.Compo[i] = "";
            self.CompoPreview.Percent[i] = "";
        }

        AppContext.Wizard.SaveCompoPreview(self.CompoPreview)
    },
