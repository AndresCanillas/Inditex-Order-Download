            self.elements.FillingWeight[0].value = ''

        }

    }
};


var LabelFitHelper = {

    joinFiber: (fiberText, fiberPercent) => {
        let positions = []
        for (let index = 0; index < fiberPercent.length; index++) {

            if (fiberPercent[index] !== "") {
                positions.push(index)
            }

            if (index == 0 && fiberPercent[index] === "") {
                positions.push(index)
            }

        }
        let fiber = ""
        let fiberRestore = []
        let fiberPercentRestore = []

        for (let index = 0; index < fiberText.length; index++) {
            if (positions.includes(index)) {
                if (fiber.length > 0) {
                    fiberRestore.push(fiber.trimStart().trimEnd());
                    fiber = ""
                }

                fiberPercentRestore.push(fiberPercent[index])

            }


            fiber = `${fiber} ${fiberText[index]}`

        }

        if (fiber.length > 0) {
            fiberRestore.push(fiber.trimStart().trimEnd())
        }

        let obj = {
            fiberRestore,
            fiberPercentRestore,
            positions
        }
        return obj
    },

    mmToPixels: (milimiters, ppi = 96) => {
        let widthInInches = milimiters / 25.4;
        let widthInPixels = widthInInches * ppi;
        return widthInPixels
    },
    // ppi = 96
    inchesToPixels: (inches, ppi = 96) => {
        
        if (ppi < 96) {
            ppi = 96
        }

        //   alert(width)
        return inches * ppi;
    },
    withoutBlankSpaces: word => {

        let stringSinEspacios = word.replace(/\s/g, '');
        return stringSinEspacios;
    },
    prefix: (word) => {

        word = word.trim()
        if (word.endsWith(" -")) {
            return word;
        } else if (word.endsWith("-")) {

            return word.substr(0, word.length - 1) + ' -'
        } else {
            return (word)
        }
    },

    sufix: (word) => {

        if (word.endsWith("- ")) {
            return word
        } else if (word.endsWith(" -")) {

            return word.substr(0, word.length - 2) + "- "
        }

        else if (word.endsWith("-")) {
            return word.substr(0, word.length - 1) + '- '
        }
        else {
            return word
        }
    },

    initLabelElement: (page, fontSize, cssClass, width = 0) => {
        const d = document
        let $element = d.createElement("div")
        $element.classList.add(cssClass)
        $element.setAttribute("data-pagenumber", page)
        $element.style.fontSize = fontSize + 'px '
        return $element

    },

    createLabelElementContainer: (text, cssClass, labelWidth = 0) => {
        let $element = document.createElement("div")
        let classes = cssClass.split(" ")
        classes.forEach(c => {

            $element.classList.add(c)
        })



        $element.textContent = text
        return $element
    },

    createFormDivElement: (inputNumberValue, page) => {

        const d = document
        let $form = d.createElement("div")
        let $inputNumber = d.createElement("input")
        $inputNumber.type = "number"
        $inputNumber.value = inputNumberValue
        $inputNumber.setAttribute("data-pagenumber", page)

