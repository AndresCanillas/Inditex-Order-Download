	var SelectPrinterDialog = function () {
		FormView.Extend(this, "@g["Select a printer"]", "/Common/SelectPrinterDialog");
	};

	SelectPrinterDialog.prototype = {
		constructor: SelectPrinterDialog,

		OnLoad: function () {
			this.LoadPrinters();
		},

		LoadPrinters: function (e) {
			var self = this;
			AppContext.Printers.GetByLocationID(this.model.LocationID, function (data) {
				AppContext.LoadComboBox(self.elements.PrinterID, data, "ID", "Name");
			});
		}
	};
//</script>